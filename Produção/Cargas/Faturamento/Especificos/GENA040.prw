Static function ProcLink()
Local cPerg		:= "GENA040"
Local cAliSql	:= GetNextAlias()
Local cDirDest	:= "C:\temp\" 
Local cNameFile	:= ""
Local cUrl		:= ""

AjusSX1(cPerg)

If !Pergunte(cPerg,.T.)
	Return nil
EndIf

BeginSql Alias cAliSql
	SELECT F2_FILIAL,F2_NFELETR,F2_DOC,F2_SERIE,F2_CODNFE FROM %Table:SF2% SF2
	WHERE F2_FILIAL = %xFilial:SF2%
	AND F2_DOC BETWEEN %Exp:MV_PAR02% AND %Exp:MV_PAR03%
	AND F2_SERIE = %Exp:MV_PAR01%
	AND F2_CODNFE <> ' '
	AND F2_ESPECIE = 'RPS'
	AND SF2.%NotDel%
EndSql

WFForceDir(cDirDest)

(cAliSql)->(DbGoTop())
While (cAliSql)->(!EOF())

	cNameFile	:= "RPS_"+AllTrim((cAliSql)->(F2_DOC+F2_SERIE))+".html"
	cUrl		:= GerLink((cAliSql)->F2_NFELETR,(cAliSql)->F2_CODNFE)
	
	MemoWrite(cDirDest+cNameFile,cUrl)
	
	(cAliSql)->(DbSkip())
EndDo

(cAliSql)->(DbCloseArea())

Return nil


