Static Function GerLink(cNumNfe,cCodNfe)

Local cLink	:= ""

cLink += '<html> '+Chr(13)+Chr(10)
cLink += '    <head> '+Chr(13)+Chr(10)
cLink += '    <script type="text/javascript"> '+Chr(13)+Chr(10)
cLink += '    function load() '+Chr(13)+Chr(10)
cLink += '    { '+Chr(13)+Chr(10)
cLink += '    window.location.href = "https://notacarioca.rio.gov.br/contribuinte/notaPrint.aspx?nf='+cNumNfe+'&inscricao=4184823&verificacao='+AllTrim(StrTran(cCodNfe,"-",""))+'" ;'+Chr(13)+Chr(10)
cLink += '    } '+Chr(13)+Chr(10)
cLink += '    </script> '+Chr(13)+Chr(10)
cLink += '    </head> '+Chr(13)+Chr(10)
cLink += '    <body onload="load()"> '+Chr(13)+Chr(10)
cLink += '</body> '+Chr(13)+Chr(10)
cLink += '</html> '+Chr(13)+Chr(10)

//cGet := HttpGet('https://notacarioca.rio.gov.br/contribuinte/notaPrint.aspx?nf=16&inscricao=4184823&verificacao=VZAKGYRE')

Return cLink