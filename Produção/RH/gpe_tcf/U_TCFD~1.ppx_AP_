__Response += "	<tr>" + CHR(13)+CHR(10)
__Response += "		<td>" + CHR(13)+CHR(10)
__Response += '			<div id="container">' + CHR(13)+CHR(10)
__Response += '				<img src="imagens/commons/img_atencao.gif" alt="Aten&ccedil;&atilde;o" />' + CHR(13)+CHR(10)
__Response += '				<p id="erro">' + CHR(13)+CHR(10)
__Response += "					<span>Prezado Colaborador.</span>" + CHR(13)+CHR(10)
__Response += "				</p>" + CHR(13)+CHR(10)
__Response += "				<br/>" + CHR(13)+CHR(10)
Return(__Response)

Static Function PlainH_2()
Local __Response := ""

__Response += '				<div class="linha"></div>' + CHR(13)+CHR(10)
